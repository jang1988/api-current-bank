{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/uploads/(.*)",
        "dest": "/uploads/$1"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/banks/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/tags/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "functions": {
        "api/**/*.js": {
          "timeout": 30
        }
      }
  }
  
